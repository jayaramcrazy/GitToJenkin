pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'App Building'
            }
        }
       stage('Test') {
            steps {
                echo 'App Testing'
            }
        }
        stage('Deploy') {
            steps {
                echo 'App Depolying'
            }
        }
        stage('Record') {
            steps {
                echo 'App Record'
            }
        }
	stage('Remove'){
	     steps {
	        echo 'App Remove'
	    }

	}
	stage('recursive'){
	     step {
	       echo 'App recursive'
	     }
	}
    }
}
